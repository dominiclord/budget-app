{
    "URL":"/",
    "dev_mode": true,
    "cache": {
        "active": false
    },
    "budget":{
    },
    "modules": {
    },
    "databases": {
        "local": {
            "hostname": "127.0.0.1",
            "username": "root",
            "password": "root",
            "database": "budget"
        }
    },
    "default_database": "local",
    "service_providers":{
        "charcoal/model/service-provider/model":{}
    },
    "logger": {
        "active": true,
        "levels": {
            "debug": true
        }
    },
    "translator":{
        "locales": {
            "languages": {
                "en": {}
            },
            "default_language": "en"
        }
    },
    "view": {
        "paths": [
            "templates/",
            "vendor/locomotivemtl/charcoal-app/templates/"
        ],
        "default_controller":"budget/template/generic",
        "default_engine": "mustache"
    },
    "metadata":{
        "paths":[
            "vendor/locomotivemtl/charcoal-app/metadata/",
            "vendor/locomotivemtl/charcoal-base/metadata/",
            "vendor/locomotivemtl/charcoal-property/metadata/",
            "metadata/"
        ]
    }
}
